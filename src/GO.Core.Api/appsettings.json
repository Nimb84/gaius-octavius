{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		},
		"TelegramBot": {
			"ChatId": "<ENV_SECRET:TelegramBot__LogChatId>",
			"Level": "Error",
			"Length": "1000"
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"GaiusBotUsersDb": "<ENV_SECRET:ConnectionStrings__GaiusBotUsersDb>",
		"GaiusBotMoviesDb": "<ENV_SECRET:ConnectionStrings__GaiusBotMoviesDb>"
	},
	"AppConfiguration": {
		"Domain": "<ENV_SECRET:AppConfiguration__Domain>"
	},
	"TelegramBotConfiguration": {
		"WebhookUrl": "/api/webhooks/telegram",
		"Token": "<ENV_SECRET:TelegramBot__Token>",
		"AdminChatId": "<ENV_SECRET:TelegramBot__AdminChatId>"
	},
	"IMDbConfiguration": {
		"ApiKey": "<ENV_SECRET:IMDb__ApiKey>"
	}
}